@{
    ViewBag.Title = "Iletisim";
}
@section baslik{
    <title>İletişim</title>
}

<div class="clearfix main_content floatleft">


    <div class="clearfix content">

        <div class="contact_us">

            <h1>İletişim</h1>

            <p>
                Her türlü sorularınızı bu iletişim sayfasında sorabilirsiniz
            </p>

            <form>
                <p><input type="text" class="wpcf7-text" placeholder="İsminiz*" /></p>
                <p><input type="text" class="wpcf7-email" placeholder="Email*" /></p>
                <p><input type="text" class="wpcf7-text" placeholder="Konu*" /></p>
                <p><textarea class="wpcf7-textarea" placeholder="Mesajınız*"></textarea></p>
                <p><input type="Submit" class="wpcf7-submit" value="Gönder" /></p>
            </form>

        </div>

    </div>


</div>
